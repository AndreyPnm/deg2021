    "tslib": "^2.0.0",
    "webpack-bundle-analyzer": "^4.1.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^11.0.0",
    "@angular-builders/jest": "^10.0.1",
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular-eslint/builder": "^0.8.0-beta.5",
