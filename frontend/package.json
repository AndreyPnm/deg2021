{
  "name": "portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0 --hmr",
    "start:anon": "ng serve anon --host=0.0.0.0 --hmr",
    "start:checkup": "ng serve checkup --host=0.0.0.0 --hmr",
    "build": "ng build",
    "build:anon": "ng build anon",
    "build:portal": "ng build portal",
    "test": "ng test --project=portal",
    "test:anon": "ng test --project=anon --coverage",
    "test:once": "ng test --project=portal --coverage",
    "test:anon:watch": "ng test --project=anon --watch --detectOpenHandles",
    "lint": "ng lint",
    "e2e": "npm run webdriver-update && ng e2e",
    "e2e:portal": "ng e2e portal",
    "e2e:anon": "ng e2e anon",
    "stats": "ng build --prod --stats-json && webpack-bundle-analyzer dist/portal/stats.json",
    "anon:ssr": "DIST=dist/anon PORT=4202 ng serve anon-ssr",
    "webdriver-update": "webdriver-manager update",
    "build:error-pages": "ng build error-pages",
    "prerender:error-pages": "ng run error-pages:prerender",
    "build:all": "npm run build:error-pages && npm run build:anon && npm run build:portal"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "node scripts/ng-lint.js"
    }
  },
  "dependencies": {
    "@angular/animations": "~11.0.2",
    "@angular/common": "~11.0.2",
    "@angular/compiler": "~11.0.2",
    "@angular/core": "~11.0.2",
    "@angular/forms": "~11.0.2",
    "@angular/platform-browser": "~11.0.2",
    "@angular/platform-browser-dynamic": "~11.0.2",
    "@angular/platform-server": "^11.0.2",
    "@angular/router": "~11.0.2",
    "@cikrf/gas-utils": "^2.0.13",
    "@datorama/akita": "^4.22.0",
    "@nguniversal/common": "^11.0.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@types/sanitizer": "0.0.28",
    "@wavesenterprise/rtk-encrypt": "^1.2.0-RC5",
    "@wavesenterprise/voting-sign-lib": "^2.2.0",
    "big-integer": "1.6.48",
    "bn.js": "^5.1.3",
    "body-parser": "^1.19.0",
    "buffer": "^6.0.3",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "express": "^4.17.1",
    "gost-crypto": "1.1.4",
    "http-proxy-middleware": "^2.0.1",
